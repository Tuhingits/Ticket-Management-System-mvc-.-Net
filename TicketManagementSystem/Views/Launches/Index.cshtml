@model IEnumerable<TicketManagementSystem.Models.Launch>

@{
    ViewData["Title"] = "Home Page";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rout)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rout)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.L_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.L_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.L_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
